{
  "version": 2,
  "builds": [
    {
      "src": "graphQL-backend/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "react-frontend/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "graphQL-backend/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "graphQL-backend/app.js"
    }
  ]
}
